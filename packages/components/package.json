    "registry": "https://labkey.jfrog.io/artifactory/api/npm/libs-client"
  },
  "homepage": "https://github.com/LabKey/labkey-ui-components#readme",
  "dependencies": {
    "@labkey/api": "1.35.0",
    "@testing-library/jest-dom": "~6.5.0",

    "@testing-library/user-event": "~14.5.2",
    "bootstrap": "~3.4.1",
    "classnames": "~2.5.1",
    "date-fns": "~3.6.0",
    "date-fns-tz": "~3.1.3",
    "enzyme": "~3.11.0",
    "font-awesome": "~4.7.0",
    "immer": "~10.1.1",
    "immutable": "~3.8.2",
    "normalizr": "~3.6.2",
    "numeral": "~2.0.6",
    "react": "~18.3.1",
    "react-beautiful-dnd": "~13.1.1",
    "react-color": "~2.19.3",
    "react-datepicker": "~4.17.0",
    "react-dom": "~18.3.1",
